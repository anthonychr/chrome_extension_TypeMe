{
	"manifest_version": 2,
	"name" : "Type Me",
	"version" : "1.0",
	"description" : "This extension for typing anything in the input box and showing on the Title",
	"icons": {

		"128": "128a.png",
		"48":"48a.png",
		"16":"16a.png"
	},
	"browser_action":{
		"default_icon":"16a.png",
		"default_popup":"popup.html"
	},

	"permissions": [
		"storage"
	]
}
